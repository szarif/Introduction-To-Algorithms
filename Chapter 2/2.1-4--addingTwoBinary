public int[] addBinary(int[] arr1, int[] arr2) {
  int[] results = new int[];

  int carry = 0;

  for(int i = 0; i < arr1.length(); ++i) {
    results[i] = (arr1[i] + arr2[i] + carry) % 2; //if results is bigger than 1 (2) than it is 0
    carry = (arr1[i] + arr2[i] + carry) / 2;  //1 if results is = to 2 else 0
  } 

  c[i] = carry;
}